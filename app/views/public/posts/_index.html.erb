<table class='table table-hover table-inverse mt-3'>
  <thead>
    <tr>
      <th>タイトル</th>
      <th>場所名</th>
      <th>タグ</th>
      <th>カテゴリ</th>
    </tr>
  </thead>
  <tbody>
    <% posts.each do |post| %>
      <tr>
        <td><%= link_to post.name, post_path(post) %></td>
        <td><%= post.place %></td>
        <td>
          <%= post.post_tag_id %>
        </td>
        <td><%= post.categories.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>