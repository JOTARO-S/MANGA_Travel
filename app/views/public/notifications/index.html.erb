<div class="container">
    <div class="row pt-3">
        <div class="col-md-12">
            <h2 class="">投稿一覧</h2>
                <% notifications = @notifications.where.not(visitor_id: current_user.id) %>
                <% if notifications.exists? %>
                  <div class='row mt-3'>
                  <% @notifications.each do |f| %>
                   
                  <% end %>
                  </div>
                  <div class="row">
                    <%= paginate notifications %>
                  </div>
                <% else %>
                  <div class='row mt-3'>
                    <h3>通知はありません。</h3>
                  </div>
                <% end %>
        </div>
    </div>
</div>